  <%= simple_form_for experience do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :category, collection: categories %>
    <%= f.input :address %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, class:"btn btn-success", style:"margin-bottom: 5px" %>
  <% end %>
