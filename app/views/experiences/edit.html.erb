<div class="container">
  <%= simple_form_for @experience do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :category, as: :select, collection: @categories%>
    <%= f.input :address %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>
</div>
